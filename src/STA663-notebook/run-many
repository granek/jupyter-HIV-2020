sudo docker run --name jupyter-sql-001 --link test-mysql:jupyter-sql-001 \
  -d -p 40001:8888 \
  -e PASSWORD="badpassword" \
  -v /srv/persistent-data/homedirs/user001:/home/jovyan/work \
  -e NB_UID=1000 \
  jupyter-sql
sleep 3
